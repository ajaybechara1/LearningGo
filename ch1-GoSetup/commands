go run hello.go
go build hello.go
go build -o hello_world hello.go
go install github.com/rakyll/hey@latest // installs the package in $GOPATH/bin
go fmt hello.go
go install golang.org/x/tools/cmd/goimports@latest
goimports -l -w . // optimise imports
brew install golangci-lint
brew upgrade golangci-lint
golangci-lint run
go mod init ch1
make
go get golang.org/dl/go.1.15.6
go1.15.6 download
go1.15.6 build
go1.15.6 env GOROOT
rm -rf $(go1.15.6 env GOROOT)
rm $(go env GOPATH)/bin/go1.15.6
